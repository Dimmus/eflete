         program {
            name: "arrow_anim_stop";
            action: ACTION_STOP;
            target: "arrow_anim_start";
            target: "arrow_anim_1";
            target: "arrow_anim_2";
            after: "arrow_anim_stop_1";
         }
         program {
            name: "arrow_anim_stop_1";
            action: STATE_SET "default" 0.0;
            target: "arrow_right";
            target: "arrow_left";
