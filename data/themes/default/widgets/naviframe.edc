      part { name: "elm.text.title";
         type: TEXT;
         repeat_events: 1;
         scale: 1;
         clip_to: "elements_clip";
         description { state: "default" 0.0;
            align: 0.5 1.0;
            fixed: 1 1;
            min: 0 18;
            max: -1 18;
            color: 223 219 210 255;
            rel1 {
               relative: 1.0 0.0;
               offset: 3 0;
               to_x: "elm.swallow.prev_btn";
            }
            rel2 {
               relative: 0.0 1.0;
               offset: -3 -1;
               to_x: "elm.swallow.bt3";
               to_y: "elm.swallow.prev_btn";
            }
            text {
               font: "PT";
               size: 12;
               size_range: 9 14;
               fit: 1 0;
               align: 0.0 1.0;
            }
         }
      }
      part { name: "elm.swallow.bt0";
         type: SWALLOW;
         clip_to: "elements_clip";
         description { state: "default" 0.0;
            align: 1.0 1.0;
            fixed: 1 1;
            min: 18 19;
            max: 18 19;
            rel2 {
               to_x: "elements_clip";
               to_y: "elm.text.title";
