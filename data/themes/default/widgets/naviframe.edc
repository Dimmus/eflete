            }
            rel2 {
               relative: 0.0 1.0;
               to: "content_clip";
            }
         }
         description { state: "next" 0.0;
            align: 0.0 0.0;
            rel1 {
               relative: 1.0 0.0;
               to: "content_clip";
            }
            rel2 {
               relative: 2.0 1.0;
               to: "content_clip";
            }
         }
      }
      part { name: "elements_clip";
         type: RECT;
         clip_to: "title_clip";
         description { state: "default" 0.0;
            rel1 {
               to: "title_clip";
            }
            rel2 {
               to: "title_clip";
            }
         }
         description { state: "previous" 0.0;
            visible: 0;
            color: 255 255 255 0;
            rel1 {
               relative: -1.0 0.0;
               to: "title_clip";
            }
            rel2 {
               relative: 0.0 1.0;
