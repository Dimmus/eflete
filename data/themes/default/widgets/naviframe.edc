            }
            rel2 {
               relative: 2.0 1.0;
               to: "title_clip";
            }
         }
      }
      part { name: "title_base";
         type: SPACER;
         description { state: "default" 0.0;
            rel1 {
               to: "elements_clip";
            }
            rel2 {
               to: "elements_clip";
            }
         }
      }
      part { name: "elm.swallow.prev_btn";
         type: SWALLOW;
         scale: 1;
         clip_to: "elements_clip";
         description { state: "default" 0.0;
            align: 0.0 0.5;
            fixed: 1 1;
            min: 30 30;
            max: 30 30;
            rel1 {
               offset: 0 5;
               to: "elements_clip";
            }
            rel2 {
               relative: 0.0 1.0;
               offset: 0 -5;
               to: "elements_clip";
            }
         }
         description { state: "hidden" 0.0;
            visible: 0;
            align: 0.0 0.5;
            fixed: 1 1;
            min: 10 30;
            max: 10 30;
            rel1 {
               offset: 0 5;
               to: "elements_clip";
            }
            rel2 {
               relative: 0.0 1.0;
               offset: 0 -5;
               to: "elements_clip";
            }
         }
      }
      part { name: "elm.text.subtitle";
         type: TEXT;
         repeat_events: 1;
         scale: 1;
         clip_to: "elements_clip";
         description { state: "default" 0.0;
            align: 0.5 1.0;
            fixed: 1 1;
            color: 183 179 170 200;
            rel1 {
               relative: 1.0 0.0;
               offset: 3 0;
               to_x: "elm.swallow.prev_btn";
            }
            rel2 {
               relative: 1.0 0.0;
               to: "elm.text.title";
            }
            text {
               font: "PT";
