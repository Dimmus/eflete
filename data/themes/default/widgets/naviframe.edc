         type: RECT;
         description { state: "default" 0.0;
            align: 0.0 0.0;
            fixed: 0 1;
            min: 1 40;
            rel2 {
               relative: 1.0 0.0;
            }
         }
         description { state: "hide" 0.0;
            visible: 0;
            align: 0.0 1.0;
            fixed: 0 1;
            min: 1 40;
            rel2 {
               relative: 1.0 0.0;
            }
         }
      }
      part { name: "content_clip";
         type: RECT;
         mouse_events: 0;
         description { state: "default" 0.0;
            rel1 {
               relative: 0.0 1.0;
               to_y: "title_clip";
            }
         }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
         scale: 1;
         clip_to: "content_clip";
         description { state: "default" 0.0;
            align: 0.0 0.0;
            rel1 {
