   }
   parts {
      part { name: "elm.picker";
         type: SWALLOW;
         description { state: "default" 0.0;
            align: 0.5 0.0;
            rel1.offset: 0 12;
            rel2.relative: 1.0 0.0;
         }
      }
      part { name: "elm.palette";
         type: SWALLOW;
         description { state: "default" 0.0;
            align: 0.5 1.0;
            rel1 {
               relative: 0.0 1.0;
               offset: 0 14;
               to_y: "elm.picker";
            }
            rel2 {
               relative: 1.0 0.0;
               offset: -1 -26;
               to_y: "elm.selector";
            }
         }
         description { state: "off" 0.0;
