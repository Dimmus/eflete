         description { state: "default" 0.0;
            rel1 {
               to: "elm.picker";
            }
            rel2 {
               to: "elm.picker";
            }
            image {
               normal: "bg_demo.png";
               middle: DEFAULT;
            }
            fill {
               type: TILE;
            }
         }
      }
      part { name: "elm.picker";
         type: SWALLOW;
         description { state: "default" 0.0;
            fixed: 1 1;
            min: 51 52;
            max: 51 52;
