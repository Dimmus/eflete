         program { name: "active";
            signal: "elm,state,active"; source: "elm";
            action: STATE_SET "active" 0.0;
            target: "elm.text";
            target: "elm.swallow.entry";
         }
         program { name: "inactive";
            signal: "elm,state,inactive"; source: "elm";
            action: STATE_SET "default" 0.0;
            target: "elm.text";
            target: "elm.swallow.entry";
         }
         program { name: "toggle_text";
            signal: "mouse,clicked,1"; source: "elm.dragable.slider";
            action: SIGNAL_EMIT "elm,action,entry,toggle" "elm";
         }
         program {
            signal: "elm,state,enabled"; source: "elm";
            action: STATE_SET "default" 0.0;
            target: "downh";
            target: "uph";
            target: "access.text";
            target: "disabler";
            target: "elm.swallow.entry";
            target: "elm.text";
         }
         program {
            signal: "elm,state,disabled"; source: "elm";
            action: STATE_SET "disabled" 0.0;
            target: "downh";
            target: "uph";
            target: "access.text";
            target: "disabler";
            target: "elm.swallow.entry";
            target: "elm.text";
         }
         FOCUS("elm,action,focus", "glow", "new");
         FOCUS("elm,action,unfocus", "glow", "default");
      }
   }

#undef BUTTON
#undef CLICK
#undef INCDEC
#undef FOCUS
#undef GLOW
