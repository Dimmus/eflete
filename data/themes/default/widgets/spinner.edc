         }
         part { name: "text.clipper";
            type: RECT;
            description { state: "default" 0.0;
               visible: 1;
               align: 0.5 0.5;
               fixed: 1 1;
               color: 0 0 0 64;
               rel1 {
                  relative: 1.0 0.0;
                  to: "downh";
