#define ITEM_HEIGHT 26
group { name: "elm/genlist/item/small/double_label/default";
   data.item: "stacking" "above";
   data.item: "selectraise" "on";
   data.item: "texts" "elm.text elm.text.sub";
   data.item: "contents" "elm.swallow.icon elm.swallow.end";
   data.item: "treesize" TREEPAD;
   images {
      image: "section-item-current-bg.png" COMP;
      image: "section-item-bg.png" COMP;
   }
   parts {
      part { name: "event";
         type: RECT;
         description { state: "default" 0.0;
            color: 0 0 0 0;
         }
      }
      part { name: "base";
         type: IMAGE;
         mouse_events: 0;
         description { state: "default" 0.0;
            image.normal: "section-item-bg.png";
            fill.smooth: 0;
         }
      }
      part { name: "bg";
         type: IMAGE;
         clip_to: "disclip";
         mouse_events: 0;
         description { state: "default" 0.0;
            visible: 0;
            image.normal: "section-item-current-bg.png";
            fill.smooth: 0;
         }
         description { state: "selected" 0.0;
            inherit: "default" 0.0;
            visible: 1;
         }
      }
      part { name: "elm.swallow.pad";
         type: SWALLOW;
         description { state: "default" 0.0;
            fixed: 1 0;
            align: 0.0 0.5;
            rel2.relative: 0.0  1.0;
         }
      }
      part { name: "elm.swallow.icon";
         clip_to: "disclip";
         type: SWALLOW;
         description { state: "default" 0.0;
            fixed: 1 0;
            align: 0.0 0.5;
            SET_SIZE(ITEM_HEIGHT, ITEM_HEIGHT);
            rel1 {
               to_x: "elm.swallow.pad";
               relative: 1.0  0.0;
            }
            rel2.to_x: "elm.swallow.pad";
         }
      }
      part { name: "swallow.end_left_top_pad";
         type:SPACER;
         description {
            SET_SIZE(5, 5);
            align: 1.0 0.0;
         }
      }
      part { name: "swallow.end_left_bot_pad";
         type:SPACER;
         description {
            SET_SIZE(5, 5);
            align: 1.0 1.0;
         }
      }
      part { name: "elm.swallow.end";
         type: SWALLOW;
         clip_to: "disclip";
         description { state: "default" 0.0;
            fixed: 1 0;
            align: 1.0 0.5;
            aspect: 1.0 1.0;
            SET_SIZE(ITEM_HEIGHT, ITEM_HEIGHT);
            rel1 {
               relative: 0.0  1.0;
               to: "swallow.end_left_top_pad";
            }
            rel2 {
               relative: 0.0  0.0;
               to: "swallow.end_left_bot_pad";
            }
         }
      }
      part { name: "text_container";
         type:SPACER;
         scale: 1;
         description{ state:"default" 0.0;
            align: 0.0 0.0;
            rel1 {
               to: "elm.swallow.icon";
               relative: 1.0 0.0;
            }
            rel2 {
               to: "elm.swallow.end";
               relative: 0.0 1.0;
            }
         }
      }
      part { name: "text_container_top_pad";
         type:SPACER;
         scale: 1;
         description{ state:"default" 0.0;
            align: 0.0 0.0;
            SET_SIZE(ITEM_HEIGHT/8, ITEM_HEIGHT/8);
            rel1.to: "text_container";
            rel2.to: "text_container";
         }
      }
      part { name: "text_container_bot_pad";
         type:SPACER;
         scale: 1;
         description{ state:"default" 0.0;
            align: 0.0 1.0;
            SET_SIZE(ITEM_HEIGHT/8, ITEM_HEIGHT/8);
            rel1.to: "text_container";
            rel2.to: "text_container";
         }
      }
      part { name: "elm.text";
         type: TEXT;
         clip_to: "disclip";
         mouse_events: 0;
         scale: 1;
         description { state: "default" 0.0;
            fixed: 1 1;
            color: 246 246 246 255;
            text {
               font: FN;
               size: 12;
               min: 0 1;
               align: 0.0 0.5;
               text_class: "list_item";
            }
            rel1 { to: "text_container_top_pad";
               relative: 1.0  1.0;
            }
            rel2 { to_x: "elm.swallow.end";
               to_y: "text_container";
               relative: 0.0  0.5;
            }
         }
         description { state: "selected" 0.0;
            inherit: "default" 0.0;
         }
      }
      part { name: "elm.text.sub";
         type: TEXT;
         clip_to: "disclip";
         mouse_events: 0;
         scale: 1;
         description { state: "default" 0.0;
            fixed: 0 1;
            color: 0 0 0 128;
            color3: 0 0 0 0;
            text {
               font: FN;
               size: 8;
               min: 1 1;
               align: 0.0 0.5;
               text_class: "list_item";
            }
            rel1 { to_x: "text_container_bot_pad";
               to_y: "text_container";
               relative: 1.0  0.5;
            }
            rel2 { to: "text_container";
               relative: 1.0  1.0;
            }
         }
         description { state: "selected" 0.0;
            inherit: "default" 0.0;
            color: 128 128 128 255;
            color3: 0 0 0 32;
         }
      }
      part { name: "disclip";
         type: RECT;
         description { state: "default" 0.0;
            rel1.to: "bg";
            rel2.to: "bg";
         }
         description { state: "disabled" 0.0;
            inherit: "default" 0.0;
            color: 255 255 255 125;
         }
      }
   }
   programs {
      program {
         name:    "go_active";
         signal:  "elm,state,selected";
         source:  "elm";
         action:  STATE_SET "selected" 0.0;
         target:  "bg";
         target:  "elm.text";
         target:  "elm.text.sub";
      }
      program {
         name:    "go_passive";
         signal:  "elm,state,unselected";
         source:  "elm";
         action:  STATE_SET "default" 0.0;
         target:  "bg";
         target:  "elm.text";
         target:  "elm.text.sub";
         transition: LINEAR 0.1;
      }
      program {
         name:    "go_disabled";
         signal:  "elm,state,disabled";
         source:  "elm";
         action:  STATE_SET "disabled" 0.0;
         target:  "disclip";
      }
      program {
         name:    "go_enabled";
         signal:  "elm,state,enabled";
         source:  "elm";
         action:  STATE_SET "default" 0.0;
         target:  "disclip";
      }
   }
}

