            visible: 1;
            fill.smooth: 0;
            image {
               normal: "treemenu-lvl1-item-current-bg.png";
               border: 0 0 1 1;
            }
         }
         description { state: "selected" 0.0;
            inherit: "default" 0.0;
            visible: 1;
            image {
               normal: "treemenu-lvl1-item-bg.png";
               border: 0 0 1 1;
            }
         }
      }
      part { name: "elm.swallow.pad";
         type: SWALLOW;
         description { state: "default" 0.0;
            fixed: 1 0;
            align: 0.0 0.5;
            rel2.relative: 0.0  1.0;
