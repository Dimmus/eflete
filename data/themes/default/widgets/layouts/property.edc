group { name: "elm/layout/live_view/swallow_text";
   parts {
      part { name: "padding_left_top";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            SET_SIZE(10, 6)
            align: 0.0 0.0;
         }
      }
      part { name: "info";
         type: SWALLOW;
         scale: 1;
         description { state: "default" 0.0;
            SET_SIZE(14, 14)
            align: 0.0 0.5;
            visible: 1;
            rel1 {
               to: "padding_left_top";
               relative: 1.0 1.0;
            }
         }
      }
      part { name: "padding_middle";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            SET_SIZE(4, 6)
            align: 0.0 0.0;
            rel1 {
               to_x: "info";
               relative: 1.0 0.0;
            }
         }
      }
      part {
         name: "elm.text";
         type: TEXT;
         scale: 1;
         mouse_events: 0;
         description {
            state: "default" 0.0;
            min: 1 16;
            align: 0.0 0.0;
            color: LIGHT_GREY_FONT_COLOR;
            rel1 {
               to_x: "padding_middle";
               to_y: "padding_left_top";
               relative: 1.0 1.0;
            }
            text {
               font: FN;
               size: 12;
               max: 1 1;
            }
         }
      }
      part { name: "padding_right_bottom";
         type: SPACER;
         description { state: "default" 0.0;
            min: 0 0;
            align: 1.0 0.0;
         }
      }
   }
}

group { name: "elm/layout/live_view/signals";
   parts {
      part { name: "text_padding_top_left";
         type: SPACER;
         scale: 1;
         description { state: "default" 0;
            align: 0 0;
            SET_SIZE(6, 6);
         }
      }
      part { name: "elm.text";
         type: TEXT;
         mouse_events: 0;
         scale: 1;
         description { state: "default" 0;
            align: 0 0.0;
            color: LIGHT_GREY_FONT_COLOR;
            rel1 {
               relative: 1 0;
               to_x: "text_padding_top_left";
               to_y: "elm.swallow.content";
            }
            rel2 {
               relative: 0 1;
               to_x: "padding";
               to_y: "elm.swallow.content";
            }
            text {
               font: FN;
               size: 12;
               min: 0 0;
               max: 1 1;
            }
         }
      }
      part { name: "content_padding_bottom_right";
         type: SPACER;
         scale: 1;
         description { state: "default" 0;
            align: 1 1;
            SET_SIZE(6, 6);
         }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
         scale: 1;
         description { state: "default" 0;
            align: 1 0.5;
            SET_SIZE(40, 20);
            rel2 {
               relative: 0 0;
               to: "content_padding_bottom_right";
            }
         }
      }
      part { name: "padding";
         type: SPACER;
         description { state: "default" 0;
            align: 1 0.5;
            min: 5 0;
            max: 5 -1;
            rel2 {
               relative: 0 1;
               to_x: "elm.swallow.content";
            }
         }
      }
   }
}

