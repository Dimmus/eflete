group { name: "elm/layout/property/1swallow";
   parts {
      part { name: "padding.top";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            align: 0.5 0.0;
            min: 0 6;
            max: -1 6;
         }
      }
      part { name: "padding.bottom";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            align: 0.5 1.0;
            min: 0 6;
            max: -1 6;
         }
      }
      part { name: "area.content";
         type: SPACER;
         description { state: "default" 0.0;
            min: 0 19;
            max: -1 19;
            rel1 {
               relative: 0.0 1.0;
               to: "padding.top";
            }
            rel2 {
               relative: 1.0 0.0;
               to: "padding.bottom";
            }
         }
      }
      part { name: "area.text";
         type: SPACER;
         description { state: "default" 0.0;
            align: 0.0 0.5;
            max: 144 -1;
            rel1 {
               to: "area.content";
            }
            rel2 {
               relative: 0.3 1.0;
               to: "area.content";
            }
         }
      }
      part { name: "elm.text";
         type: TEXT;
         description { state: "default" 0.0;
            align: 0.0 0.5;
            color: LIGHT_GREY_FONT_COLOR;
            rel1 {
               to: "area.text";
            }
            rel2 {
               to: "area.text";
            }
            text {
               font: FN;
               size: 12;
               max: 1 1;
            }
         }
      }
      part { name: "padding.content";
         type: SPACER;
         description { state: "default" 0.0;
            min: 30 0;
            max: 30 -1;
            align: 0.0 0.5;
            rel1 {
               relative: 1.0 0.0;
               to_x: "area.text";
               to_y: "area.content";
            }
            rel2 {
               to: "area.content";
            }
         }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
         description { state: "default" 0.0;
            rel1 {
               relative: 1.0 0.0;
               to_x: "padding.content";
               to_y: "area.content";
            }
            rel2 {
               to: "area.content";
            }
         }
      }
   }
}

