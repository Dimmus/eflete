group { name: "ewe/tabs/horizontal/default";
   parts {
      part { name: "bg";
         type: RECT;
         description { state: "default" 0.0;
            color: TABS_BG_COLOR;
         }
      }
      part { name: "ewe.text";
         type: TEXT;
         description { state: "default" 0.0;
            align: 0.0 0.5;
            max: 0 -1;
            rel2 {
               to_y: "tabs_area";
            }
            text {
               font: PT;
               size: 13;
            }
         }
         description { state: "visible" 0.0;
            align: 0.0 0.5;
            max: 112 -1;
            rel2 {
               to_y: "tabs_area";
            }
            text {
               font: PT;
               size: 13;
            }
         }
      }
      part { name: "tabs_area";
         type: SWALLOW;
         description { state: "default" 0.0;
            align: 0.0 0.0;
            min: 0 22;
            max: -1 22;
            rel1 {
               relative: 1.0 0.0;
               offset: 2 6;
               to_x: "ewe.text";
            }
            rel2 {
               offset: -3 0;
            }
         }
      }
      part { name: "separator";
         type: RECT;
         description { state: "default" 0.0;
            align: 0.5 1.0;
            max: -1 1;
            color: 25 25 26 255;
            rel1 {
               to_y: "tabs_area";
            }
            rel2 {
               to_y: "tabs_area";
            }
         }
      }
      part { name: "content_area";
         type: SWALLOW;
         description { state: "default" 0.0;
            rel1 {
               relative: 0.0 1.0;
               to_y: "tabs_area";
            }
         }
      }
   }
   programs {
      program { name: "text_show";
         signal: "ewe,state,text,visible";
         source: "ewe";
         action: STATE_SET "visible" 0.00;
         target: "ewe.text";
      }
      program { name: "text_hide";
         signal: "ewe,state,text,hidden";
         source: "ewe";
         action: STATE_SET "default" 0.00;
         target: "ewe.text";
      }
   }
}

group { name: "ewe/tabs/vertical/default";
   parts {
      part { name: "bg";
         type: RECT;
         description { state: "default" 0.0;
            color: TRANSPARENT_COLOR;
         }
      }
      part { name: "tabs_area";
         type: SWALLOW;
         clip_to: "tabs_area_clip";
         description { state: "default" 0.0;
            align: 0.0 0.0;
            min: 140 29;
            max: 140 29;
            rel1.offset: 0 -2;
         }
      }
      part { name: "tabs_area_clip";
         type: RECT;
         description { state: "default" 0.0;
            rel1.to: "tabs_area";
            rel2.to: "tabs_area";
         }
      }
      part { name: "content_area";
         type: SWALLOW;
         description { state: "default" 0.0;
            rel1 {
               to_x: "tabs_area";
               relative: 1.0 0.0;
            }
         }
      }
   }
}

group {name: "ewe/tabs/item/head/horizontal/default";
   images {
      image: "tab-head-active-bg.png" COMP;
      image: "tab-head-bg.png" COMP;
   }
   parts {
      part { name: "bg";
         type: IMAGE;
         description { state: "default" 0.0;
            min: 120 22;
            max: -1 22;
            image {
               normal: "tab-head-bg.png";
               border: 5 5 10 10;
            }
            rel1.offset: 1 0;
            rel2.offset: -2 0;
         }
         description { state: "active" 0.0;
            inherit: "default" 0.0;
            image.normal: "tab-head-active-bg.png";
         }
      }
      part { name: "ewe.icon";
         type: SWALLOW;
         description { state: "default" 0.0;
            min: 0 16;
            max: 0 16;
            visible: 0;
            fixed: 0 1;
            align: 0.0 0.5;
            rel1 {
               to: "bg";
               offset: 2 2;
            }
            rel2 {
               to: "bg";
               offset: -3 -3;
            }
         }
         description { state: "visible" 0.0;
            inherit: "default" 0.0;
            min: 16 16;
            max: 16 16;
            visible: 1;
         }
      }
      part { name: "ewe.text";
         type: TEXT;
         description { state: "default" 0.0;
            color: TABS_TITLE_COLOR;
            fixed: 0 1;
            text {
               font: FN;
               size: 13;
               min: 1 1;
               ellipsis: -1;
            }
            rel1 {
               to: "ewe.icon";
               relative: 1.0 0.0;
               offset: 2 2;
            }
            rel2 {
               to: "ewe.swallow.end";
               offset: -3 -3;
               relative: 0.0 1.0;
            }
         }
         description { state: "active" 0.0;
            inherit: "default" 0.0;
            color: TABS_DISABLED_TITLE_COLOR;
         }
      }
      part { name: "events";
         type: RECT;
         description { state: "default" 0.0;
            color: TRANSPARENT_COLOR;
         }
      }
      part { name: "ewe.swallow.end";
         type: SWALLOW;
         description { state: "default" 0.0;
            min: 0 16;
            max: 0 16;
            visible: 0;
            fixed: 1 1;
            align: 1.0 0.5;
            rel1 {
               to: "bg";
               offset: 2 2;
            }
            rel2 {
               to: "bg";
               offset: -3 -3;
            }
         }
         description { state: "visible" 0.0;
            inherit: "default" 0.0;
            min: 16 16;
            max: 16 16;
            visible: 1;
         }
      }
      part { name: "disabler";
         type: RECT;
         description { state: "default" 0.0;
            color: TABS_DISABLER_COLOR;
            visible: 0;
            rel1.to: "bg";
            rel2.to: "bg";
         }
         description { state: "disabled" 0.0;
            inherit: "default" 0.0;
            visible: 1;
         }
      }
   }
   programs {
      program { name: "activate";
         signal: "ewe,tabs,item,activate";
         source: "ewe";
         action: STATE_SET "active" 0.0;
         target: "bg";
         target: "ewe.text";
      }
      program { name: "deactivate";
         signal: "ewe,tabs,item,deactivate";
         source: "ewe";
         action: STATE_SET "default" 0.0;
         target: "bg";
         target: "ewe.text";
      }
      program { name: "icon_show";
         signal: "ewe,tabs,item,icon,show";
         source: "ewe";
         action: STATE_SET "visible" 0.0;
         target: "ewe.icon";
      }
      program { name: "icon_hide";
         signal: "ewe,tabs,item,icon,hide";
         source: "ewe";
         action: STATE_SET "default" 0.0;
         target: "ewe.icon";
      }
      program { name: "swallow_show";
         signal: "ewe,tabs,item,swallow,show";
         source: "ewe";
         action: STATE_SET "visible" 0.0;
         target: "ewe.swallow.end";
      }
      program { name: "swallow_hide";
         signal: "ewe,tabs,item,swallow,hide";
         source: "ewe";
         action: STATE_SET "default" 0.0;
         target: "ewe.swallow.end";
      }
      program { name: "disable";
         signal: "ewe,tabs,item,disable";
         source: "ewe";
         action: STATE_SET "disabled" 0.0;
         target: "disabler";
      }
      program { name: "enable";
         signal: "ewe,tabs,item,enable";
         source: "ewe";
         action: STATE_SET "default" 0.0;
         target: "disabler";
      }
   }
}

