#define PADDING_SIZE_SET \
   min: 2 2; \
   max: 2 2;

group {name: "ewe/tabs/item/content/horizontal/default";
   images {
      image: "tab-content-bg.png" COMP;
   }
   parts {
      part { name: "bg";
         type: IMAGE;
         description { state: "default" 0.0;
            image {
               normal: "tab-content-bg.png";
               border: 1 1 1 1;
            }
            fill.smooth: 0;
         }
      }
      part { name: "padding.top_left";
         type: SPACER;
         description {
            align: 0.0 0.0;
            PADDING_SIZE_SET;
         }
      }
      part { name: "padding.right_bottom";
         type: SPACER;
         description {
            align: 1.0 1.0;
            PADDING_SIZE_SET;
         }
      }
      part { name: "ewe.content";
         type: SWALLOW;
         description { state: "default" 0.0;
            rel1 {
               to: "padding.top_left";
               relative: 1.0 1.0;
            }
            rel2 {
               to: "padding.right_bottom";
               relative: 0.0 0.0;
            }
         }
      }
      part { name: "disabler";
         type: RECT;
         description { state: "default" 0.0;
            color: TABS_DISABLER_COLOR;
            visible: 0;
            rel1.to: "bg";
            rel2.to: "bg";
         }
         description { state: "disabled" 0.0;
            inherit: "default" 0.0;
            visible: 1;
         }
      }
   }
   programs {
      program { name: "disable";
         signal: "ewe,tabs,item,disable";
         source: "ewe";
         action: STATE_SET "disabled" 0.0;
         target: "disabler";
      }
      program { name: "enable";
         signal: "ewe,tabs,item,enable";
         source: "ewe";
         action: STATE_SET "default" 0.0;
         target: "disabler";
      }
   }
}

#undef PADDING_SIZE

