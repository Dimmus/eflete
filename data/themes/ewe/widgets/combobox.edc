group { name: "ewe/combobox/expander/default";
   parts{
      part { name: "bg";
         type: RECT;
         description { state: "default" 0.0;
            color: 52 53 55 255;
         }
      }
      part { name: "top.offset";
         type: SPACER;
         description { state: "default";
            align: 0.0 0.0;
            min: 2 2;
            max: 2 2;
            rel1.to: "bg";
         }
      }
      part { name: "bot.offset";
         type: SPACER;
         description { state: "default";
            align: 1.0 1.0;
            min: 2 2;
            max: 2 2;
            rel2.to: "bg";
         }
      }
      part { name: "swallow.part";
         type: SWALLOW;
         description { state: "default";
            align: 0.0 0.0;
            rel1 {
               to: "top.offset";
               relative: 1.0 1.0;
            }
            rel2 {
               to: "bot.offset";
               relative: 0.0 0.0;
            }
         }
      }
   }
}

group {name: "ewe/combobox/item/content/default";
   parts {
      part { name: "ewe.bg";
         type: RECT;
         mouse_events: 1;
         description { state: "default" 0.0;
            color: TRANSPARENT_COLOR;
         }
         description { state: "mousein" 0.0;
            color: COMBOBOX_ITEM_MOUSEIN_BG;
         }
      }
      part { name: "left.offset";
         type: SPACER;
         description { state: "default" 0.0;
            align: 0.0 0.0;
            min:  COMBOBOX_ITEM_PADDING;
            max:  COMBOBOX_ITEM_PADDING;
            rel1 {
               relative: 0.0 0.0;
               to: "ewe.bg";
            }
         }
      }
      part { name: "right.offset";
         type: SPACER;
         description { state: "default" 0.0;
            align: 1.0 1.0;
            fixed: 1 1;
            min:  COMBOBOX_ITEM_RIGHT_PADDING;
            max:  COMBOBOX_ITEM_RIGHT_PADDING;
            rel1 {
               relative: 1.0 1.0;
               to: "ewe.bg";
            }
         }
      }
      part { name: "ewe.text";
         type: TEXT;
         description { state: "default" 0.0;
            min: 0 20;
            max: -1 20;
            align: 0.0 0.5;
            color: COMBOBOX_ITEM_TEXT_COLOR;
            text {
               font: FN;
               min: 1 0;
               elipsis: -1;
               size: 12;
               align: 0.0 0.5;
            }
            rel1 {
               to: "left.offset";
               relative: 1.0 1.0;
            }
            rel2 {
               to: "right.offset";
               relative: 0.0 0.0;
            }
         }
      }
   }
   programs {
      program { name: "mouse.in";
         signal: "mouse,in";
         source: "ewe.*";
         action: STATE_SET "mousein" 0.0;
         target: "ewe.bg";
      }
      program { name: "mouse.out";
         signal: "mouse,out";
         source: "ewe.*";
         action: STATE_SET "default" 0.0;
         target: "ewe.bg";
      }
      program { name: "clicked";
         signal: "mouse,clicked,1";
         source: "ewe.*";
         action: SIGNAL_EMIT "select" "ewe";
      }
   }
}

