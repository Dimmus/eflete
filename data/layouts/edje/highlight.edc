         type: RECT;
         scale: 1;
         mouse_events: 1;
         description {
            state: "default" 0.0;
            color: 0 0 0 0;
         }
      }
      part {
         name: "border";
         type: IMAGE;
         mouse_events: 1;
         description {
            state: "default" 0.0;
            visible: 1;
            rel1.to: "base";
            rel2.to: "base";
            image {
               normal: "handler.png";
               border: 2 2 2 2;
               middle: NONE;
            }
         }
      }
   }
}
