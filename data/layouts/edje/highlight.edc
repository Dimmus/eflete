group {
   images {
      image: "highlight.png" COMP;
   }
   name: "eflete/highlight/border/default";
   parts {
      part {
         name: "base";
         type: RECT;
         scale: 1;
         mouse_events: 1;
         repeat_events: 1;
         description {
            state: "default" 0.0;
            color: 0 0 0 0;
         }
      }
      part {
         name: "bg";
         type: SWALLOW;
         scale: 1;
         mouse_events: 1;
         description {
            state: "default" 0.0;
            visible: 1;
            rel1.to: "base";
            rel2.to: "base";
         }
      }
      part {
         name: "border";
         type: IMAGE;
         mouse_events: 1;
         description {
            state: "default" 0.0;
            visible: 1;
            rel1.to: "base";
            rel2.to: "base";
            image {
               normal: "highlight.png";
               border: 1 1 1 1;
               middle: NONE;
            }
         }
      }
   }
}
group {
