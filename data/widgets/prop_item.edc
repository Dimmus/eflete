         scale: 1;
         description {
            state: "default" 0.0;
            min: 5 5;
            max: 5 5;
         }
      }
      part {
         name: "field1.swallow";
         type: SWALLOW;
         scale: 1;
         description {
            state: "default" 0.0;
            rel2 {
               to_x: "separator";
               relative: 0.0 1.0;
            }
         }
      }
      part {
         name: "field2.swallow";
         type: SWALLOW;
         scale: 1;
         description {
            state: "default" 0.0;
            rel1 {
               to_x: "separator";
               relative: 1.0 0.0;
            }
         }
      }
   }
}
